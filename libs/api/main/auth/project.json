{
  "root": "libs/api/main/auth",
  "sourceRoot": "libs/api/main/auth/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/api/main/auth/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/api/main/auth"],
      "options": {
        "jestConfig": "libs/api/main/auth/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:api", "scope:main", "scope:auth", "tag:lib"]
}
